    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "prisma": "^6.15.0",
    "puppeteer": "^24.17.1",
    "redis": "^5.8.2"
  },
  "devDependencies": {
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.1",
